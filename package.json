{
  "name": "@kupibilet/icons",
  "version": "1.0.0",
  "description": "icons for kupibilet.ru",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf ./dist",
    "svgo": "svgo --enable=removeUselessStrokeAndFill -f ./src/ -o ./dist/icons",
    "sprite": "svg-sprite -l -D=dist -s --ss=icons.svg --symbol-dest='' ./src/*.svg",
    "list": "node ./scripts/list.js",
    "build": "npm run clean && npm run svgo && npm run list",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kupibiletdev/icons.git"
  },
  "author": "Kupibilet team (http://kupibilet.ru)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kupibiletdev/icons/issues"
  },
  "homepage": "https://github.com/kupibiletdev/icons#readme",
  "devDependencies": {
    "eslint-config-kupibilet": "^4.0.2",
    "gh-pages": "^0.11.0",
    "svg-sprite": "^1.3.3",
    "svgo": "^0.6.6",
    "webpack": "^1.13.1"
  }
}
